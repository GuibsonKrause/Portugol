algoritmo "semnome"

var
numero, intervalo, contador: real
inicio
// Seção de Comandos 
para intervalo de 1 ate 80 faca
escreval("informe um número:")
leia(numero)
se numero >= 10 entao
se numero <= 150 entao
contador <- contador + 1
fimse
fimse
escreval("Foram digitados",contador,"números no intervalo entre 10 e 150.")
fimalgoritmo
